    "GOOGLE_DRIVE_CLIENT_ID": "your_google_drive_client_id_here",
    "GOOGLE_DRIVE_CLIENT_SECRET": "your_google_drive_client_secret_here",
    
    "DEFAULT_AVATAR_PROVIDER": "heygen",
    "VEED_API_KEY": "your_veed_api_key_here",
    "SIMLI_API_KEY": "your_simli_api_key_here",
    "ELEVENLABS_API_KEY": "your_elevenlabs_api_key_here"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/blog/:slug",
      "destination": "/blog/[slug]"
    }
  ]
}
